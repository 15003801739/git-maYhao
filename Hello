public static void main(String[] args) {
        int [] array =new int[9];
        int [] array1 =new int[9];
        printArray(array,array1);
    }
	public static void printArray(int[]array,int[] array1)
	{
		for(int i=1;i<10;i++) {
		   for(int j=1;j<=i;j++) {
		System.out.println(i+"*"+j+"="+(i*j)+"\t");
	}
	System.out.println();
}
}
